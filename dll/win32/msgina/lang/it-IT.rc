    COMBOBOX IDC_REASON_LIST, 39, 117, 190, 95, CBS_DROPDOWNLIST | WS_VSCROLL | WS_TABSTOP
    LTEXT "", IDC_REASON_DESCRIPTION, 39, 136, 190, 27
    LTEXT "&Comment:", IDC_STATIC, 39, 166, 80, 8
    EDITTEXT IDC_REASON_COMMENT, 39, 176, 190, 35, ES_MULTILINE | WS_TABSTOP
    DEFPUSHBUTTON "OK", IDOK, 150, 228, 55, 14, WS_GROUP
    PUSHBUTTON "Annulla", IDCANCEL, 211, 228, 55, 14

    // DEFPUSHBUTTON "OK", IDOK, 87, 228, 55, 14, WS_GROUP
    // PUSHBUTTON "Annulla", IDCANCEL, 150, 228, 55, 14
    // PUSHBUTTON "&Aiuto", IDHELP, 211, 228, 55, 14
END

