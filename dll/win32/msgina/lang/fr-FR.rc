IDD_SHUTDOWN_REASON DIALOGEX 0, 0, 275, 249
STYLE DS_SHELLFONT | DS_MODALFRAME | DS_CENTER | WS_POPUPWINDOW | WS_CAPTION
CAPTION "Arrêt de ReactOS"
FONT 8, "MS Shell Dlg"
BEGIN
    ICON IDI_SHUTDOWN, IDC_STATIC, 9, 9, 20, 20, WS_GROUP
    LTEXT "&Que voulez-vous que l'ordinateur fasse ?", IDC_STATIC, 39, 9, 167, 10
    COMBOBOX IDC_SHUTDOWN_ACTION, 39, 22, 190, 95, CBS_DROPDOWNLIST | WS_VSCROLL | WS_TABSTOP
    LTEXT "", IDC_SHUTDOWN_DESCRIPTION, 39, 45, 190, 27
    GROUPBOX "Moniteur d'événements de Mise hors tension", IDC_STATIC, 9, 74, 257, 146
    LTEXT "Sélectionnez l'option qui décrit le mieux la raison pour laquelle vous voulez arrêter l'ordinateur.", IDC_STATIC, 39, 86, 190, 17
    LTEXT "&Option :", IDC_STATIC, 39, 106, 80, 8
    AUTOCHECKBOX "&Planifié", IDC_REASON_PLANNED, 182, 106, 47, 10
    COMBOBOX IDC_REASON_LIST, 39, 117, 190, 95, CBS_DROPDOWNLIST | WS_VSCROLL | WS_TABSTOP
    LTEXT "", IDC_REASON_DESCRIPTION, 39, 136, 190, 27
    LTEXT "&Commentaire :", IDC_STATIC, 39, 166, 80, 8
    EDITTEXT IDC_REASON_COMMENT, 39, 176, 190, 35, ES_MULTILINE | WS_TABSTOP
    DEFPUSHBUTTON "OK", IDOK, 150, 228, 55, 14, WS_GROUP
    PUSHBUTTON "Annuler", IDCANCEL, 211, 228, 55, 14

    // DEFPUSHBUTTON "OK", IDOK, 87, 228, 55, 14, WS_GROUP
    // PUSHBUTTON "Annuler", IDCANCEL, 150, 228, 55, 14
    // PUSHBUTTON "&Aide", IDHELP, 211, 228, 55, 14
END

