    AUTOCHECKBOX "I ", IDC_ADVAPPEARANCE_FONTITALIC, 230, 200, 14, 13, BS_PUSHLIKE
    PUSHBUTTON "Cancelar", IDCANCEL, 191, 220, 50, 14
    DEFPUSHBUTTON "Aceptar", IDOK, 137, 220, 50, 14
END

