IDM_CONSOLE_SMALL MENU
BEGIN
    POPUP "&File"
    BEGIN
        MENUITEM "&Nový\tCtrl+N", IDM_FILE_NEW
        MENUITEM "&Otevřít...\tCtrl+O", IDM_FILE_OPEN
        MENUITEM SEPARATOR
        MENUITEM "&Ukončit\tAlt+F4", IDM_FILE_EXIT
    END
    POPUP "&Soubor"
    BEGIN
        MENUITEM "&About ReactOS Management Console", IDM_HELP_ABOUT
    END
END

IDM_CONSOLE_LARGE MENU
BEGIN
    POPUP "&File"
    BEGIN
        MENUITEM "&Nový\tCtrl+N", IDM_FILE_NEW
        MENUITEM "&Otevřít...\tCtrl+O", IDM_FILE_OPEN
        MENUITEM "&Save", IDM_FILE_SAVE
        MENUITEM "Uložit j&ako...", IDM_FILE_SAVEAS
        MENUITEM SEPARATOR
        MENUITEM "&Ukončit\tAlt+F4", IDM_FILE_EXIT
    END
    POPUP "&Okna"
    BEGIN
        MENUITEM "List", IDM_WINDOWS_LIST
    END
    POPUP "&Soubor"
    BEGIN
        MENUITEM "&About ReactOS Management Console", IDM_HELP_ABOUT
    END
END

