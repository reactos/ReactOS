LANGUAGE LANG_CHINESE_TRADITIONAL, SUBLANG_CHINESE_TRADITIONAL

IDM_CONSOLE_SMALL MENU
BEGIN
    POPUP "&File"
    BEGIN
        MENUITEM "新建(&N)\tCtrl+N", IDM_FILE_NEW
        MENUITEM "開啟...(&O)\tCtrl+O", IDM_FILE_OPEN
        MENUITEM SEPARATOR
        MENUITEM "結束(&X)\tAlt+F4", IDM_FILE_EXIT
    END
    POPUP "檔案(&F)"
    BEGIN
        MENUITEM "&About ReactOS Management Console", IDM_HELP_ABOUT
    END
END

IDM_CONSOLE_LARGE MENU
BEGIN
    POPUP "&File"
    BEGIN
        MENUITEM "新建(&N)\tCtrl+N", IDM_FILE_NEW
        MENUITEM "開啟...(&O)\tCtrl+O", IDM_FILE_OPEN
        MENUITEM "&Save", IDM_FILE_SAVE
        MENUITEM "另存為...(&A)", IDM_FILE_SAVEAS
        MENUITEM SEPARATOR
        MENUITEM "結束(&X)\tAlt+F4", IDM_FILE_EXIT
    END
    POPUP "窗口(&W)"
    BEGIN
        MENUITEM "List", IDM_WINDOWS_LIST
    END
    POPUP "檔案(&F)"
    BEGIN
        MENUITEM "&About ReactOS Management Console", IDM_HELP_ABOUT
    END
END

STRINGTABLE
BEGIN
    IDS_APPTITLE "ReactOS Management Console"
    IDS_CONSOLETITLE "Console%1!u!"
END
