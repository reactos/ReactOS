LANGUAGE LANG_JAPANESE, SUBLANG_JAPANESE_JAPAN

IDM_CONSOLE_SMALL MENU
BEGIN
    POPUP "&File"
    BEGIN
        MENUITEM "新規(&N)\tCtrl+N", IDM_FILE_NEW
        MENUITEM "開く(&O)\tCtrl+O", IDM_FILE_OPEN
        MENUITEM SEPARATOR
        MENUITEM "ペイントの終了(&Q)\tAlt+F4", IDM_FILE_EXIT
    END
    POPUP "ファイル(&F)"
    BEGIN
        MENUITEM "&About ReactOS Management Console", IDM_HELP_ABOUT
    END
END

IDM_CONSOLE_LARGE MENU
BEGIN
    POPUP "&File"
    BEGIN
        MENUITEM "新規(&N)\tCtrl+N", IDM_FILE_NEW
        MENUITEM "開く(&O)\tCtrl+O", IDM_FILE_OPEN
        MENUITEM "&Save", IDM_FILE_SAVE
        MENUITEM "名前を付けて保存(&A)...", IDM_FILE_SAVEAS
        MENUITEM SEPARATOR
        MENUITEM "ペイントの終了(&Q)\tAlt+F4", IDM_FILE_EXIT
    END
    POPUP "ウィンドウ(&W)"
    BEGIN
        MENUITEM "List", IDM_WINDOWS_LIST
    END
    POPUP "ファイル(&F)"
    BEGIN
        MENUITEM "&About ReactOS Management Console", IDM_HELP_ABOUT
    END
END

STRINGTABLE
BEGIN
    IDS_APPTITLE "ReactOS Management Console"
    IDS_CONSOLETITLE "Console%1!u!"
END
