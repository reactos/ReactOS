        MENUITEM "&Aç...\tDenetim+O", IDM_FILE_OPEN
        MENUITEM SEPARATOR
        MENUITEM "&Çıkış\tSeçenek+F4", IDM_FILE_EXIT
    END
    POPUP "&Kütük"
    BEGIN
        MENUITEM "&About ReactOS Management Console", IDM_HELP_ABOUT
    END
END

