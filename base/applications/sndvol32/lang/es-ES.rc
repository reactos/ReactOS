        MENUITEM "P&ropiedades", IDC_PROPERTIES
        MENUITEM "&Controles avanzados", IDC_ADVANCED_CONTROLS
        MENUITEM SEPARATOR
        MENUITEM "&Salir", IDC_EXIT
