IDM_MAINMENU MENU
BEGIN
    POPUP "&Možnosti"
    BEGIN
        MENUITEM "&Vlastnosti", IDC_PROPERTIES
        MENUITEM "Rozšírené &ovládanie", IDC_ADVANCED_CONTROLS
        MENUITEM SEPARATOR
        MENUITEM "&Skončiť", IDC_EXIT
    END
    POPUP "&Pomocník"
    BEGIN
        MENUITEM "&Témy Pomocníka", IDC_HELP_TOPICS
        MENUITEM SEPARATOR
        MENUITEM "Č&o je Ovládanie hlasitosti", IDC_ABOUT
    END
END

STRINGTABLE
BEGIN
