LANGUAGE LANG_CHINESE_TRADITIONAL, SUBLANG_CHINESE_TRADITIONAL

IDR_MAINMENU MENU
BEGIN
    POPUP "檔案(&F)"
    BEGIN
        MENUITEM "開啟...(&O)\tCtrl+O", IDM_OPEN_FILE
        MENUITEM "關閉(&C)\tCtrl+W", IDM_CLOSE_FILE
        MENUITEM SEPARATOR
        MENUITEM "退出(&X)", IDM_EXIT
    END
    POPUP "播放(&P)"
    BEGIN
        MENUITEM "播放/暫停(&P)\tCtrl+P", IDC_PLAY
        MENUITEM "停止(&O)\tCtrl+S", IDC_STOP
        MENUITEM SEPARATOR
        MENUITEM "重複(&T)\tCtrl+T", IDM_REPEAT
    END
    POPUP "檢視(&V)"
    BEGIN
        MENUITEM "單窗模式(&M)", IDM_SWITCHVIEW
    END
    POPUP "設備(&D)"
    BEGIN
        MENUITEM SEPARATOR
        MENUITEM "屬性(&P)", IDM_DEVPROPS
        MENUITEM SEPARATOR
        MENUITEM "音量控制(&V)", IDM_VOLUMECTL
    END
    POPUP "檔案(&F)"
    BEGIN
        MENUITEM "關於註冊表編輯器(&A)", IDM_ABOUT
    END
END

ID_ACCELERATORS ACCELERATORS
BEGIN
    "^O", IDM_OPEN_FILE
    "^W", IDM_CLOSE_FILE
    "^P", IDC_PLAY
    "^S", IDC_STOP
    "^T", IDM_REPEAT
END

