    PUSHBUTTON "Pārvietot uz &augšu", IDC_BTN_MOVE_UP_BOOT_OPTION, 190, 65, 70, 12
    PUSHBUTTON "Pārvietot uz &leju", IDC_BTN_MOVE_DOWN_BOOT_OPTION, 280, 65, 70, 12
