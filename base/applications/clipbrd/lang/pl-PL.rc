LANGUAGE LANG_POLISH, SUBLANG_DEFAULT

ID_ACCEL ACCELERATORS
BEGIN
    VK_DELETE, CMD_DELETE, VIRTKEY
END

MAIN_MENU MENU
BEGIN
    POPUP "&Plik"
    BEGIN
        MENUITEM "&Otwórz...", CMD_OPEN
        MENUITEM "Zapisz &jako...", CMD_SAVE_AS
        MENUITEM SEPARATOR
        MENUITEM "Za&kończ", CMD_EXIT
    END
    POPUP "&Edycja"
    BEGIN
        MENUITEM "&Usuń\tDel", CMD_DELETE
    END
    POPUP "&Widok"
    BEGIN
        MENUITEM "&Automatycznie", CMD_AUTOMATIC
    END
    POPUP "&Pomoc"
    BEGIN
        MENUITEM "&Tematy Pomocy", CMD_HELP, GRAYED
        MENUITEM SEPARATOR
        MENUITEM "&Informacje", CMD_ABOUT
    END
END

